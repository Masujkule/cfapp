
<div class="container">
  <div class="row">
    <div class="col-md-6 col-xs-12 contact" >
      <h1>contact details</h1>
      <p>address</p>
      <p>
       1 Queenscliff Rd
       Queenscliff NSW </p>
        
<iframe width="80%" height="250em" frameborder="0" style="border: 1px solid black" src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJmYYfOv2qEmsRzASaFFwZUUw
&key=AIzaSyBtZqTSqByUkNH6QYT9cQxLXw3G_iyjWYo" allowfullscreen></iframe> 
          
    </div>  <!--end col-->  

    <div class="col-md-6 col-xs-12 contact_us">
      <h1>contact us</h1>

      <%= flash[:notice] %>

      <%= form_tag("/static_pages/thank_you") do %>

      <div class="form-group">
        <%= label_tag :name %>
        <%= text_field_tag :name, nil, class: "form-control", placeholder: "Enter Name" %>
      </div>      
      <div class="form-group">
        <%= label_tag :email, "Email Address" %>
        <%= text_field_tag :email, nil, class: "form-control", placeholder: "Enter Email" %>
      </div>
      <div class="form-group">
        <%= label_tag :message %>
        <%= text_area_tag :message, nil, class: "form-control", placeholder: "Enter Message" %>
      </div> 
        <%= submit_tag "Send", class:"btn-submit"  %>   
 
      <% end %>

  </div>   <!--end col-->
 </div>   <!--end row-->
</div>  <!--end container-->
